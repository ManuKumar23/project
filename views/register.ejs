<%- include('partials/header') %>
<body>
    <main>
        <div class="box" id="heading">
            <h1>Register</h1>
        </div>
        <div class="box">
            <form class="edit" action="/register" method="POST">
                <label for="email">Email</label>
                <input type="email" placeholder="example@gmail.com" name="username" required>
                <label for="password">Password</label>
                <input type="password" placeholder="**********" name="password" required>
                <br>
                <button type="submit" class="eventBtn">Register</button>
            </form>
        </div>
    </main>
</body>
</html>